root:
    dict:
        units:
            list:
                dict:
                    people:
                        list:
                            dict:
                                tasks:
                                    list: 
                                        attributes: &task
                                            name: str
                                            description: str
                                            priority: int
                                            from: date
                                            to: date
                                            effort: float
                                            load: float
                                            absence: bool
                                jira:
                                    list: 
                                        attributes: *task
                                        cache: true
                                        handlers: 
                                            load: jira_load_task
                                    handlers: 
                                        load: jira_load_list
                                    cache: true
                                planning:
                                    render:
                                        function: show_tasks                                                                                    
                    planning:
                        render:
                            function: show_unit_tasks
